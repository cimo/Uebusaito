{
    "mappings": [
        {
            "src": "Resources/public/scss",
            "dest": "Resources/public/css"
        }
    ],
    "ignores": ["*.css"],
    "options": {
        "outputStyle": "compact",
        "lineComments": true,
        "debugInfo": false,
        "sourceMap": true,
        "unixNewlines": false,
        "autoprefix": false,
        "autoprefixConfig" : "> 1%, last 2 versions, Firefox ESR, Opera 12.1"
    },
    "customOptions": [],
    "includePaths": [],
    "requireLibs": []
}