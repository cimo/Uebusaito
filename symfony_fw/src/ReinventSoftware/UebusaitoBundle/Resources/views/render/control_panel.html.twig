    {% if is_granted("ROLE_USER") == true %}
        <li>
            <a href="{{path("root_render", {'_locale': urlLocale, 'urlCurrentPageId': urlCurrentPageId, 'urlExtra': "cp_profile"})}}">
                <i class="fa fa-user"></i> {% trans %}control_panel_1{% endtrans %}
            </a>
        </li>
        <li>
            <a href="{{path("root_render", {'_locale': urlLocale, 'urlCurrentPageId': urlCurrentPageId, 'urlExtra': "cp_payment"})}}">
                <i class="fa fa-credit-card-alt"></i> {% trans %}control_panel_2{% endtrans %}
            </a>
        </li>
    {% endif %}
    {% if is_granted("ROLE_ADMIN") == true or is_granted("ROLE_MODERATOR") == true %}
        <li>
            <a href="{{path("root_render", {'_locale': urlLocale, 'urlCurrentPageId': urlCurrentPageId, 'urlExtra': "cp_page"})}}">
                <i class="fa fa-file-text"></i> {% trans %}control_panel_3{% endtrans %}
            </a>
        </li>
        <li>
            <a href="{{path("root_render", {'_locale': urlLocale, 'urlCurrentPageId': urlCurrentPageId, 'urlExtra': "cp_user"})}}">
                <i class="fa fa-users"></i> {% trans %}control_panel_4{% endtrans %}
            </a>
        </li>
        <li>
            <a href="{{path("root_render", {'_locale': urlLocale, 'urlCurrentPageId': urlCurrentPageId, 'urlExtra': "cp_module"})}}">
                <i class="fa fa-modx"></i> {% trans %}control_panel_5{% endtrans %}
            </a>
        </li>
        <li>
            <a href="{{path("root_render", {'_locale': urlLocale, 'urlCurrentPageId': urlCurrentPageId, 'urlExtra': "cp_role"})}}">
                <i class="fa fa-address-card"></i> {% trans %}control_panel_6{% endtrans %}
            </a>
        </li>
