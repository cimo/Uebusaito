            <div class="margin_top">
                <a class="button_custom" href="{{path("root_render", {'_locale': urlLocale, 'urlCurrentPageId': urlCurrentPageId, 'urlExtra': "cp_role/create"})}}">{% trans %}button_1{% endtrans %}</a>
                <a class="button_custom" href="{{path("root_render", {'_locale': urlLocale, 'urlCurrentPageId': urlCurrentPageId, 'urlExtra': "cp_role/modify"})}}">{% trans %}button_5{% endtrans %}</a>
            </div>
            
            {% if urlExtra == "cp_role/create" %}
                {{-render(controller("UebusaitoBundle:ControlPanel\\RoleUser:creation", {'_locale': urlLocale, 'urlCurrentPageId': urlCurrentPageId, 'urlExtra': urlExtra}))-}}
            {% endif %}
            
            {% if urlExtra == "cp_role/modify" %}
                {{-render(controller("UebusaitoBundle:ControlPanel\\RoleUser:selection", {'_locale': urlLocale, 'urlCurrentPageId': urlCurrentPageId, 'urlExtra': urlExtra}))-}}
            {% endif %}
        {% endif %}
    {% elseif urlExtra == "cp_setting" %}
        {% if is_granted("ROLE_ADMIN") == true %}
            <p>{% trans %}control_panel_14{% endtrans %}</p>
            
            {{-render(controller("UebusaitoBundle:ControlPanel\\Setting:save", {'_locale': urlLocale, 'urlCurrentPageId': urlCurrentPageId, 'urlExtra': urlExtra}))-}}
        {% endif %}
    {% endif %}
