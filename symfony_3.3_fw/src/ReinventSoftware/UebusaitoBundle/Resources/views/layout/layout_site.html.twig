<!DOCTYPE html>
<html lang="{{app.request.getLocale()}}">
    <head>
        <title>{{websiteName}}</title>
        {% block headMeta %}{% endblock %}
        {% block headLink %}{% endblock %}
    </head>
    <body class="user_select_none">
        {% block bodyIncludeTop %}{% endblock %}
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    {% block headerContainer %}{% endblock %}
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 display_desktop">
                    {% block columnLeftContainer %}{% endblock %}
                </div>
                <div class="col-md-8">
                    {% block columnCenterContainer %}{% endblock %}
                </div>
                <div class="col-md-2 display_desktop">
                    {% block columnRightContainer %}{% endblock %}
                </div>
            </div>
        </div>
        <footer class="footer_fix">
            {% block footerContainer %}{% endblock %}
        </footer>
        {% block bodyIncludeDown %}{% endblock %}
        {% block javascript %}{% endblock %}
    </body>
</html>